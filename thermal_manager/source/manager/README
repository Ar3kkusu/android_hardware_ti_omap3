# How to compile C code and generate libthermal-manager.so shared library
make all

# How to copy libthermal-manager.so to android file system
cp libthermal-manager.so $MYDROID/out/target/product/blaze/system/lib/
cd $MYDROID
adb shell stop; adb sync; adb shell start

adb push $HOME/workspace/android_libconfig/libs/armeabi/libconfig.so ./system/lib
adb push libthermal-manager.so ./system/lib
adb push libthermal-manager.cfg ./system/bin
